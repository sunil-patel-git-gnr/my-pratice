  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "create-tables",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "create-dev-tables"
      ],
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
  {
    "type": "node",
    "request": "launch",
    "name": "drop-tables",
    "runtimeExecutable": "npm",
    "runtimeArgs": [
      "run-script",
      "drop-dev-tables"
    ],
    "port": 9229,
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
    {
      "command": "npm start",
      "name": "Run npm start",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/server.js"
    }
  ]
}